datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Menu {
  id    Int    @id @default(autoincrement())
  nama  String
  harga Float
}

model User {
  id    Int    @id @default(autoincrement())
  username String
  email String
  password String
}
